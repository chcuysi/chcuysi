<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="RefundMenuDao">

   <select id="getRefundMenuList" resultType="vo.RefundMenuVO" parameterType="hashmap" >
      SELECT d.dnum, d.onum, d.daddr, d.dok, d.ddate, r.rok
         FROM DELIVERY d
         LEFT JOIN REFUND r ON d.onum = r.onum
   </select>  
</mapper>