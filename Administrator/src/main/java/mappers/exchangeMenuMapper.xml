<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ExchangeMenuDao">

   <select id="getExchangeMenuList" resultType="vo.ExchangeMenuVO" parameterType="hashmap" >
      SELECT d.dnum, d.onum, d.daddr, d.dok, d.ddate, e.eok
         FROM DELIVERY d
         LEFT JOIN EXCHANGE e ON d.onum = e.onum
   </select>  
</mapper>